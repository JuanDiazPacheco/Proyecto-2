<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="347.0" prefWidth="552.0" style="-fx-background-color: #1EB2A6;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.operaciones.admin.ClientesListaController">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="339.0" prefWidth="552.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="clientesTable" prefHeight="262.0" prefWidth="321.0" style="-fx-background-color: #D4F8E8;">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="76.0" prefWidth="552.0" spacing="40.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnRegresar" mnemonicParsing="false" onAction="#handleRegresarAction" prefHeight="40.0" prefWidth="79.0" style="-fx-background-color: #FFA34D;" text="Regresar" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            <Button fx:id="btnVerMas" mnemonicParsing="false" onAction="#handleVerAction" prefHeight="40.0" prefWidth="103.0" style="-fx-background-color: #FFA34D;" text="Ver Cliente" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
